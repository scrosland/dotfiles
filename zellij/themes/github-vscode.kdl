themes {
    github-vscode-light-default {
        fg "#0969da"        // "textLink.foreground": "#0969da"
        bg "#f6f8fa"        // "panel.background": "#f6f8fa",
        black "#24292f"
        red "#cf222e"
        green "#116329"
        yellow "#4d2d00"
        blue "#0969da"
        magenta "#8250df"
